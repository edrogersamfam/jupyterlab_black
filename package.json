{
  "name": "@ryantam626/jupyterlab_black",
  "version": "0.1.0",
  "description": "A JupyterLab extension to apply Black formatter to code within codecell.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/ryantam626/jupyterlab_black",
  "bugs": {
    "url": "https://github.com/ryantam626/jupyterlab_black/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Ryan Tam",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryantam626/jupyterlab_black.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w",
    "prepare": "npm run clean && npm run build"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.16.0",
    "@jupyterlab/apputils": "^0.16.4",
    "@jupyterlab/notebook": "^0.16.3",
    "@jupyterlab/terminal": "^0.16.3"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.9.2"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
